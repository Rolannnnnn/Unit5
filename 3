//BSCS 1-B

#include <stdio.h>
int base, exponent, ans;
int i = 1;

int Power(base, exponent) {
    ans = 1;
    while (i <= exponent) {
        ans *= base;
        i++;
    }
    return ans;
}

int main () {
    int x, y, z;
    printf("Enter base, then exponent. \n");
    scanf("%d %d", &x, &y);
    z = Power(x, y);
    printf("Answer: %d", z);
}
